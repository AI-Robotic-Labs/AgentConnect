
{
  "version": "1.0",
  "module_name": "sample_protocol",
  "definitions": [
    {
      "type": "class",
      "class": {
        "name": "Requester",
        "description": "A class to send requests based on the sample protocol",
        "interfaces": [
          {
            "type": "function",
            "function": {
              "name": "send_request",
              "description": "Send a generic request and receive the response",
              "parameters": {
                "type": "object",
                "properties": {
                  "endpoint": {
                    "type": "string",
                    "description": "The API endpoint to send the request to"
                  },
                  "payload": {
                    "type": "object",
                    "description": "The data to be sent in the request",
                    "additionalProperties": true
                  },
                  "headers": {
                    "type": "object",
                    "description": "HTTP headers for the request",
                    "properties": {
                      "Authorization": {
                        "type": "string",
                        "description": "Authorization token"
                      },
                      "Content-Type": {
                        "type": "string",
                        "description": "Content type of the payload, e.g., application/json"
                      }
                    },
                    "required": ["Authorization", "Content-Type"]
                  }
                },
                "required": ["endpoint", "payload", "headers"]
              },
              "returns": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "integer",
                    "description": "HTTP status code"
                  },
                  "data": {
                    "type": "object",
                    "description": "The response data",
                    "additionalProperties": true
                  },
                  "error_message": {
                    "type": "string",
                    "description": "Error message, only if code is not 200"
                  }
                },
                "required": ["code"]
              }
            }
          }
        ]
      }
    }
  ]
}