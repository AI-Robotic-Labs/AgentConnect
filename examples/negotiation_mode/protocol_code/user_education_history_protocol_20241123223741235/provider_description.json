{
  "version": "1.0",
  "module_name": "user_education_history_protocol",
  "definitions": [
    {
      "type": "class",
      "class": {
        "name": "ProtocolProvider",
        "description": "Protocol provider class for handling user education history retrieval",
        "interfaces": [
          {
            "type": "function",
            "function": {
              "name": "set_protocol_callback",
              "description": "Set async callback function for handling protocol messages related to user education history",
              "parameters": {
                "type": "object",
                "properties": {
                  "callback": {
                    "type": "function",
                    "description": "Async function that processes protocol messages for retrieving user education history",
                    "parameters": {
                      "type": "object",
                      "properties": {
                        "userId": {
                          "type": "string",
                          "description": "Unique identifier of the user whose education history is being retrieved"
                        },
                        "includeDetails": {
                          "type": "boolean",
                          "description": "Indicates whether to include detailed educational information",
                          "default": false
                        },
                        "page": {
                          "type": "integer",
                          "description": "Page number for paginated results",
                          "minimum": 1
                        },
                        "pageSize": {
                          "type": "integer",
                          "description": "Number of records per page for pagination",
                          "minimum": 1,
                          "default": 10
                        }
                      },
                      "required": ["userId"]
                    },
                    "returns": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "integer",
                          "description": "HTTP status code indicating the result of the request"
                        },
                        "educationHistory": {
                          "type": "array",
                          "description": "List of education history records",
                          "items": {
                            "type": "object",
                            "properties": {
                              "institution": {
                                "type": "string",
                                "description": "Name of the educational institution"
                              },
                              "major": {
                                "type": "string",
                                "description": "Major field of study"
                              },
                              "degree": {
                                "type": "string",
                                "enum": ["Bachelor", "Master", "Doctorate"],
                                "description": "Type of degree obtained"
                              },
                              "achievements": {
                                "type": "string",
                                "description": "Achievements obtained during study"
                              },
                              "startDate": {
                                "type": "string",
                                "format": "date",
                                "description": "Start date of the educational period"
                              },
                              "endDate": {
                                "type": "string",
                                "format": "date",
                                "description": "End date of the educational period"
                              }
                            },
                            "required": ["institution", "major", "degree", "startDate", "endDate"]
                          }
                        },
                        "pagination": {
                          "type": "object",
                          "description": "Pagination information for the result set",
                          "properties": {
                            "currentPage": {
                              "type": "integer",
                              "description": "The current page number of the result set"
                            },
                            "pageSize": {
                              "type": "integer",
                              "description": "The number of records per page"
                            },
                            "totalPages": {
                              "type": "integer",
                              "description": "The total number of pages available"
                            },
                            "totalRecords": {
                              "type": "integer",
                              "description": "The total number of records available"
                            }
                          },
                          "required": ["currentPage", "pageSize", "totalPages", "totalRecords"]
                        },
                        "error": {
                          "type": "object",
                          "description": "Error details if the request was not successful",
                          "properties": {
                            "code": {
                              "type": "integer",
                              "description": "Error code indicating the type of error"
                            },
                            "message": {
                              "type": "string",
                              "description": "Error message providing details about the error"
                            }
                          },
                          "required": ["code", "message"]
                        }
                      },
                      "required": ["code"]
                    }
                  }
                },
                "required": ["callback"]
              }
            }
          }
        ]
      }
    }
  ]
}